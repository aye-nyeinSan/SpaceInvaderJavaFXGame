<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="250.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.invaders.controller.menuController">
    <children>
      <ImageView fitHeight="274.0" fitWidth="284.0" layoutX="-14.0" layoutY="-22.0">
         <image>
            <Image url="@com/example/invaders/assets/winspace.jpg" />
         </image>
      </ImageView>
        <Button layoutX="34.0" layoutY="142.0" mnemonicParsing="false" onAction="#onStartOver" text="Start Over">
            <font>
                <Font name="Agency FB" size="18.0" />
            </font>
        </Button>
        <Button layoutX="144.0" layoutY="142.0" mnemonicParsing="false" onAction="#onHome" prefHeight="27.0" prefWidth="71.0" text="Home">
            <font>
                <Font name="Agency FB" size="18.0" />
            </font>
        </Button>
        <Text fill="WHITE" layoutX="82.0" layoutY="116.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Score">
            <font>
                <Font name="Agency FB Bold" size="24.0" />
            </font>
        </Text>
        <Text fx:id="scoreVar" fill="WHITE" layoutX="144.0" layoutY="116.0" strokeType="OUTSIDE" strokeWidth="0.0">
            <font>
                <Font name="Agency FB" size="24.0" />
            </font>
        </Text>
        <Button layoutX="91.0" layoutY="193.0" mnemonicParsing="false" onAction="#onQuitGameConsole" prefHeight="27.0" prefWidth="71.0" text="Quit">
            <font>
                <Font name="Agency FB" size="18.0" />
            </font>
        </Button>
        <ImageView fitHeight="143.0" fitWidth="157.0" layoutX="46.0" layoutY="-18.0">
            <image>
                <Image url="@com/example/invaders/assets/win-removebg-preview.png" />
            </image>
        </ImageView>
    </children>
</AnchorPane>
