<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="250.0" prefWidth="250.0" style="-fx-background-color: LightGreen;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.invaders.controller.menuController">
    <children>
        <Label layoutX="63.0" text="You Won" textFill="WHITE">
            <font>
                <Font name="Niagara Engraved" size="48.0" />
            </font>
        </Label>
        <Button layoutX="76.0" layoutY="108.0" mnemonicParsing="false" onAction="#onStartOver" text="Start Over">
            <font>
                <Font name="Agency FB" size="18.0" />
            </font>
        </Button>
        <Button layoutX="82.0" layoutY="150.0" mnemonicParsing="false" onAction="#onHome" prefHeight="27.0" prefWidth="71.0" text="Home">
            <font>
                <Font name="Agency FB" size="18.0" />
            </font>
        </Button>
        <Text fill="#3d50ad" layoutX="82.0" layoutY="87.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Score">
            <font>
                <Font name="Agency FB" size="24.0" />
            </font>
        </Text>
        <Text fx:id="scoreVar" fill="#3d50ad" layoutX="144.0" layoutY="88.0" strokeType="OUTSIDE" strokeWidth="0.0">
            <font>
                <Font name="Agency FB" size="24.0" />
            </font>
        </Text>
        <Button layoutX="82.0" layoutY="193.0" mnemonicParsing="false" onAction="#onQuitGameConsole" prefHeight="27.0" prefWidth="71.0" text="Quit">
            <font>
                <Font name="Agency FB" size="18.0" />
            </font>
        </Button>
    </children>
</AnchorPane>